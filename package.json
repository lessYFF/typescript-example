{
  "name": "typescript-example",
  "author": "Bruce yff",
  "version": "v1.0.0",
  "private": true,
  "description": "some examples of typescript",
  "keywords": [
    "webpack",
    "typescript",
    "commitizen"
  ],
  "script": {
    "start": "node start.js",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lessYFF/typescript-example.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^6.26.3",
    "babel-preset-latest-node": "^0.4.0",
    "babel-preset-stage-3": "^6.24.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "standard-version": "^4.4.0",
    "webpack": "^4.28.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
