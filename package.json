{
  "name": "vue-webpack-template",
  "author": "Bruce yff",
  "version": "v1.0.0",
  "private": true,
  "description": "vue template using webpack4",
  "keywords": [
    "vue",
    "webpack4",
    "typescript",
    "commitizen"
  ],
  "scripts": {
    "release": "standard-version",
    "dev": "cross-env NODE_ENV=development node build/index",
    "build": "cross-env NODE_ENV=production node build/index"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lessYFF/vue-webpack-template.git"
  },
  "dependencies": {
    "vue": "^2.5.21",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "express": "^4.16.4",
    "happypack": "^5.0.1",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "nodemon-webpack-plugin": "^4.0.7",
    "open-browser-webpack-plugin": "0.0.5",
    "standard-version": "^4.4.0",
    "ts-loader": "^5.3.3",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
